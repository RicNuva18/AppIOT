<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.smartparking">
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar">
        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key" />

        <activity android:name=".FeedbackTres"></activity>
        <activity android:name=".FeedbackDos" />
        <activity android:name=".Registrarse" />
        <activity android:name=".RutaTres" />
        <activity android:name=".Reminder" />
        <activity android:name=".RutaDos" />
        <activity android:name=".FeedbackUno" />
        <activity android:name=".RutaUno" />
        <activity android:name=".ParqueaderosDisponibles" />
        <activity android:name=".SeleccionarZonaParqueo" />
        <activity android:name=".porteria" />
        <activity android:name=".SeleccionarDestino" />
        <activity android:name=".QueDeseoHacer" />
        <activity android:name=".IniciarSesion" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>